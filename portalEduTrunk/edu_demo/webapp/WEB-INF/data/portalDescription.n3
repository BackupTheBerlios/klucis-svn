@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mtest: <http://www.webkursi.lv/schema/mtest#> .
@prefix portal: <http://www.webkursi.lv/schema/portal#> .


portal:fixedPageset
    rdf:type mtest:PageSet ;
    mtest:name "welcome.do" ;
    mtest:rootComponent portal:fixedWelcomeComponent ;
    mtest:ruleList [
        a rdf:Seq ;
    ] .
             
portal:fixedWelcomeComponent
    rdf:type mtest:RootComponent ;
    mtest:viewName "welcome" ;
    mtest:title "Mtest.lv - Lietotaajs" ;   
    mtest:hasComponents [
        a rdf:Bag ;
        rdf:_1 [        
            mtest:modelKey "navigation" ;
            mtest:component portal:navigation 
        ] ;        
        rdf:_2 [        
            mtest:modelKey "logo" ;
            mtest:component portal:logo 
        ] ;  
        rdf:_3 [        
            mtest:modelKey "welcome_content" ;
            mtest:component portal:welcome_content 
        ] ;     
        rdf:_4 [        
            mtest:modelKey "footer" ;
            mtest:component portal:footer 
        ]      
    ] .    

portal:navigation
    rdf:type mtest:TopComponent ;
    mtest:viewName "navigation" .

portal:logo
    rdf:type mtest:TopComponent ;
    mtest:viewName "logo" .

portal:welcome_content
    rdf:type mtest:TopComponent ;
    mtest:viewName "welcome_content" .

portal:footer
    rdf:type mtest:TopComponent ;
    mtest:viewName "footer" .







portal:fixedUserPageset
    rdf:type mtest:PageSet ;
    mtest:name "user_new.do" ;
    mtest:rootComponent portal:fixedUserComponent ;
    mtest:ruleList [
        a rdf:Seq ;
    ] .


portal:fixedUserComponent
    rdf:type mtest:RootComponent ;
    mtest:viewName "user/user" ;
    mtest:title "Jauns Lietotaajs" ;   
    mtest:hasComponents [
        a rdf:Bag ;
        rdf:_1 [        
            mtest:modelKey "navigation" ;
            mtest:component portal:navigation 
        ] ;        
        rdf:_2 [        
            mtest:modelKey "logo" ;
            mtest:component portal:logo 
        ] ;  
        rdf:_3 [        
            mtest:modelKey "new_content" ;
            mtest:component portal:new_content 
        ] ;     
        rdf:_4 [        
            mtest:modelKey "footer" ;
            mtest:component portal:footer 
        ]      
    ] .
      

portal:new_content 
    rdf:type mtest:TopComponent ;
    mtest:viewName "user/user_new_content" .   