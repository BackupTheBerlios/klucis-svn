@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mars: <http://www.hpl.hp.com/schema/mars#> .
@prefix portal: <http://www.hpl.hp.com/schema/portal#> .


portal:fixedPageset
    rdf:type mars:PageSet ;
    mars:name "/welcome.do" ;
    mars:rootComponent portal:fixedWelcomeComponent ;
    mars:ruleList [
        a rdf:Seq ;
    ] .
             
portal:fixedWelcomeComponent
    rdf:type mars:RootComponent ;
    mars:viewName "welcome" ;
    mars:title "Mtest.lv - Lietotājs" ;   
    mars:hasComponents [
        a rdf:Bag ;
        rdf:_1 [        
            mars:modelKey "navigation" ;
            mars:component portal:navigation 
        ] ;        
        rdf:_2 [        
            mars:modelKey "logo" ;
            mars:component portal:logo 
        ] ;  
        rdf:_3 [        
            mars:modelKey "welcome_content" ;
            mars:component portal:welcome_content 
        ] ;     
        rdf:_4 [        
            mars:modelKey "footer" ;
            mars:component portal:footer 
        ]      
    ] .    

portal:navigation
    rdf:type mars:TopComponent ;
    mars:viewName "navigation" .

portal:logo
    rdf:type mars:TopComponent ;
    mars:viewName "logo" .

portal:welcome_content
    rdf:type mars:TopComponent ;
    mars:viewName "welcome_content" .

portal:footer
    rdf:type mars:TopComponent ;
    mars:viewName "footer" .







portal:fixedUserPageset
    rdf:type mars:PageSet ;
    mars:name "/user_new.do" ;
    mars:rootComponent portal:fixedUserComponent ;
    mars:ruleList [
        a rdf:Seq ;
    ] .


portal:fixedUserComponent
    rdf:type mars:RootComponent ;
    mars:viewName "user/user" ;
    mars:title "Jauns Lietotājs" ;   
    mars:hasComponents [
        a rdf:Bag ;
        rdf:_1 [        
            mars:modelKey "navigation" ;
            mars:component portal:navigation 
        ] ;        
        rdf:_2 [        
            mars:modelKey "logo" ;
            mars:component portal:logo 
        ] ;  
        rdf:_3 [        
            mars:modelKey "new_content" ;
            mars:component portal:new_content 
        ] ;     
        rdf:_4 [        
            mars:modelKey "footer" ;
            mars:component portal:footer 
        ]      
    ] .
      

portal:new_content 
    rdf:type mars:TopComponent ;
    mars:viewName "user/user_new_content" .   