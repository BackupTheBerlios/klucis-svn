@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix tiles: <http://www.mtest.lv/schema/20061008/lego#> .
@prefix : <http://example.com/bildes#> .

:bilde_11a
  a tiles:Box ;
  tiles:row [
    a rdf:Seq ;
    rdf:_1 [
      a tiles:SquareBox ;
      tiles:hasLabel "A" ; # izsecina, ka apakshaa
      tiles:hasContent :blueTriangle 
    ] ;
    rdf:_2 [
      a tiles:SquareBox ;
      tiles:hasLabel "A" ; # izsecina, ka apakshaa
      tiles:hasContent :greenArrowHead 
    ]
  ] .

:bilde_11d
  a tiles:Box ;
  tiles:row [
    a rdf:Seq ;
    rdf:_1 [
      a tiles:SquareBox ;
      tiles:hasContent [
        tiles:item :blueTriangle ;
	tiles:turn "+90"^^xsd:integer 
      ]
    ] ;
    rdf:_1 [
      a tiles:SquareBox ;
      tiles:hasContent [
        tiles:item :greenArrowHead ;
	tiles:turn "+90"^^xsd:integer 
      ]
    ] 
  ] .

:blueTriangle 
  a tiles:Image ;
  tiles:src "blueTriangle.gif" .

:greenArrowHead
  a tiles:Image ;
  tiles:src "grenArrowHead.gif" .